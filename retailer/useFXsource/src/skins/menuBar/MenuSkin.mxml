<!--- The wireframe skin class for the border of the MX Menu component. 

      @see mx.controls.Menu

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
                        >
    
    <s:states>
        <s:State name="normal"/>
        <s:State name="disabled"/>
    </s:states>
    
    <!--- @private -->
	<s:Rect id="border" left="-2" right="-2" top="-2" bottom="-2">
		<s:fill>
			<s:SolidColor id="bgFill" color="#272727"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke id="borderStroke" caps="none" color="#1b1b1b" joints="miter" miterLimit="4"
								weight="1"/>
		</s:stroke>
		<s:filters>
			<s:DropShadowFilter alpha="0.2" angle="45" blurX="3" blurY="3" color="#000000"
								distance="3" hideObject="false" inner="false" knockout="false"
								quality="2" strength="1"/>
		</s:filters>
	</s:Rect>
	<s:Rect left="-1" right="-1" top="-1" bottom="-1">
		<s:stroke>
			<s:SolidColorStroke caps="none" color="#383838" joints="miter" miterLimit="4"
								weight="1"/>
		</s:stroke>
	</s:Rect>
</s:Skin>